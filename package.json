        "find-cache-dir": "^3.3.1",
        "fs-extra": "^10.0.0",
        "global-dirs": "^3.0.0",
        "@wanwu/mitm-proxy": "^1.0.0",
        "iconv-lite": "^0.6.3",
        "is-glob": "^4.0.3",
        "is-plain-obj": "^4.0.0",
        "lodash.flatten": "^4.4.0",
        "killable": "^1.0.1",
        "koa": "^2.13.1",
        "koa-send": "^5.0.1",
        "lodash-es": "^4.17.21",
        "lru-cache": "^6.0.0",
        "micromatch": "^4.0.4",
        "mime-types": "^2.1.33",
        "mkdirp": "^1.0.4",
        "mobile-detect": "^1.4.5",
